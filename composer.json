{
    "name": "rachidlaasri/travel",
    "description": "Framework agnostic PHP package to control the time.",
    "keywords": [
        "carbon",
        "time",
        "travel"
    ],
    "homepage": "https://github.com/rashidlaasri/travel",
    "license": "MIT",
    "authors": [
        {
            "name": "Rachid Laasri",
            "email": "rashidlaasri@gmail.com",
            "homepage": "https://rachidlaasri.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "nesbot/carbon": ">=1.29.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2|^9.1"
    },
    "autoload": {
        "psr-4": {
            "RachidLaasri\\Travel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RachidLaasri\\Travel\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
